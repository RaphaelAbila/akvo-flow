{{#view FLOW.ReportsListAppletView}}
      <div class="generatedExports">
        <h1>{{t _generated_reports}}</h1>
        <a {{action "exportNewReport" target="this"}} class="button exportNewBtn">{{t _export_reports}}</a>
      </div>
      <section class="exportContainer">
        {{#if FLOW.reportsControl.reportsAvailable}}
        <!-- Here is the list of exports generated-->
        <ul class="exportList">
          {{#each report in FLOW.reportsControl}}{{#with report}}
          <li class="{{reportType reportType}} {{reportStatus state}}">
            <div class="exportInfo">
              <a href="{{filename}}">

                <h5>
                  {{reportTypeString reportType}} - <span class="exportFileName">{{reportFilename filename}}</span>
                  <span class="submissionsDate">
                    {{#if lastCollectionOnly}}
                    {{t _recent_submissions}}
                    {{else}}
                    {{#if startDate}}{{date1 startDate}} -{{/if}}{{#if endDate}}- {{date1 endDate}}{{/if}} {{t _submissions}}
                    {{/if}}
                  </span>
                </h5>

              </a>
              <span class="surveyPath">{{surveyPath formId}}</span>
            </div>
            <div class="exportStatus">
              <span class="loading">{{t _generating}}</span>
              <span class="exportDate">{{#if lastUpdateDateTime}}{{date1 lastUpdateDateTime}}{{/if}}</span>
            </div>
          </li>
          {{/with}}{{/each}}
        </ul>
        <!-- End -->
        {{else}}
        <!-- This is the message you get if you have no exports -->
        <div class="noExportMsg">
          <h3>{{t _no_exports_yet}}</h3>
          <p>{{t _click_new_export}}</p>
        </div>
        <!-- End -->
        {{/if}}
      </section>
{{/view}}
